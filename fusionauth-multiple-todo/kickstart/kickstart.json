    "todo2ApplicationId": "e9fdb985-8675-4e01-9d73-ac2d60d1dc8b",
    "apiKey": "this_really_should_be_a_long_random_alphanumeric_value_but_this_still_works",
    "asymmetricKeyId": "#{UUID()}",
    "defaultTenantId": "d7d09513-a3f5-401c-9685-34ab6c552453",
    "adminEmail": "admin@example.com",
    "adminPassword": "password",
    "todo1UserId": "#{UUID()}",
    "todo1UserEmail": "richard@example.com",
    "todo1UserPassword": "password",
    "todo2UserId": "#{UUID()}",
    "todo2UserEmail": "dinesh@example.com",
    "todo2UserPassword": "password"
  },
  "apiKeys": [
    {
      "key": "#{apiKey}",
      "description": "Unrestricted API key"
    }
  ],
  "requests": [
    {
      "method": "POST",
      "url": "/api/key/generate/#{asymmetricKeyId}",
      "tenantId": "#{defaultTenantId}",
      "body": {
        "key": {
          "algorithm": "RS256",
          "name": "For exampleapp",
          "length": 2048
        }
      }
    },
    {
      "method": "PATCH",
      "url": "api/system-configuration",
      "body": {
        "systemConfiguration": {
          "corsConfiguration": {
            "allowCredentials": true,
            "allowedMethods": [
              "GET",
              "POST",
              "OPTIONS"
            ],
            "allowedOrigins": [
              "http://localhost:4200",
              "http://localhost:5173"
            ],
            "debug": false,
            "enabled": true,
            "preflightMaxAgeInSeconds": 0
          }
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/registration",
      "body": {
        "user": {
          "email": "#{adminEmail}",
          "password": "#{adminPassword}"
        },
        "registration": {
          "applicationId": "#{FUSIONAUTH_APPLICATION_ID}",
          "roles": [
            "admin"
          ]
        }
      }
    },
    {
      "method": "PATCH",
      "url": "/api/tenant/#{defaultTenantId}",
      "body": {
        "tenant": {
          "issuer": "http://localhost:9011"
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/application/#{todo1ApplicationId}",
      "tenantId": "#{defaultTenantId}",
      "body": {
        "application": {
          "name": "TODO_1",
          "oauthConfiguration": {
            "authorizedRedirectURLs": [
              "http://localhost:4200"
            ],
            "authorizedOriginURLs": [
              "http://localhost:4200"
            ],
            "clientSecret": "super-secret-secret-that-should-be-regenerated-for-production",
            "logoutURL": "http://localhost:4200",
            "enabledGrants": [
              "authorization_code",
              "refresh_token"
            ],
            "debug": true,
            "generateRefreshTokens": true,
            "requireRegistration": true
          },
          "jwtConfiguration": {
            "enabled": true,
            "accessTokenKeyId": "#{asymmetricKeyId}",
            "idTokenKeyId": "#{asymmetricKeyId}"
          },
          "registrationConfiguration": {
            "enabled": true
          }
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/registration/#{todo1UserId}",
      "body": {
        "user": {
          "birthDate": "1985-11-23",
          "email": "#{todo1UserEmail}",
          "firstName": "Richard",
          "lastName": "Hendricks",
          "password": "#{todo1UserPassword}"
        },
        "registration": {
          "applicationId": "#{todo1ApplicationId}"
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/application/#{todo2ApplicationId}",
      "tenantId": "#{defaultTenantId}",
      "body": {
        "application": {
          "name": "TODO_2",
          "oauthConfiguration": {
            "authorizedRedirectURLs": [
              "http://localhost:5173"
            ],
            "authorizedOriginURLs": [
              "http://localhost:5173"
            ],
            "clientSecret": "super-secret-secret-that-should-be-regenerated-for-production",
            "logoutURL": "http://localhost:5173/logged-out",
            "enabledGrants": [
              "authorization_code",
              "refresh_token"
            ],
            "debug": true,
            "generateRefreshTokens": true,
            "requireRegistration": true
